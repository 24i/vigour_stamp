{
  "name": "vigour-stamp",
  "version": "1.0.1",
  "description": "generates unique stamps for changes, possiblity to observe progress of stamps",
  "main": "lib/index.js",
  "scripts": {
    "test": "node test | tap-difflet && standard",
    "watch": "nodemon test | tap-difflet",
    "cover": "istanbul cover --report none --print detail test/index.js",
    "docs": "node_modules/vigour-doc/bin/vdoc",
    "travis": "npm run cover -s && istanbul report lcov && ((cat coverage/lcov.info | coveralls) || exit 0) && standard"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vigour-io/stamp.git"
  },
  "keywords": [
    "change",
    "stamp"
  ],
  "dependencies": {
    "babelify": "^7.3.0",
    "babel-preset-es2015": "^6.6.0"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ]
        }
      ]
    ]
  },
  "devDependencies": {
    "tape": "^4.4.0",
    "tap-difflet": "0.4.0",
    "vigour-performance": "^1.0.0",
    "standard": "^7.0.1",
    "nodemon": "^1.9.1",
    "coveralls": "^2.11.9",
    "istanbul": "^0.4.3",
    "vigour-doc": "^1.0.5",
    "standard": "^7.0.1",
    "pre-commit": "^1.1.3"
  },
  "author": "jim de beer",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vigour-io/stamp/issues"
  },
  "homepage": "https://github.com/vigour-io/stamp#readme"
}
