{
  "name": "brisky-stamp",
  "version": "3.0.8",
  "description": "Generate unique meta information for change in a system",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "scripts": {
    "build": "rollup -c",
    "watch": "rollup -c -w",
    "prepublish": "npm run build",
    "pretest": "npm run build",
    "test": "node test | tap-difflet && standard",
    "dev": "nodemon test | tap-difflet",
    "cover": "istanbul cover --report none --print detail test/index.js",
    "travis": "npm run cover -s && istanbul report lcov && ((cat coverage/lcov.info | coveralls) || exit 0) && standard"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vigour-io/stamp.git"
  },
  "keywords": [
    "change",
    "stamp",
    "meta information"
  ],
  "files": [
    "dist",
    "package.json"
  ],
  "devDependencies": {
    "coveralls": "^2.11.9",
    "istanbul": "^0.4.3",
    "nodemon": "^1.9.1",
    "pre-commit": "^1.1.3",
    "rollup": "^0.37.0",
    "rollup-watch": "^2.5.0",
    "standard": "^8.4.0",
    "tap-difflet": "0.4.0",
    "tape": "^4.4.0"
  },
  "author": "jim de beer",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vigour-io/stamp/issues"
  },
  "homepage": "https://github.com/vigour-io/stamp#readme",
  "dependencies": {
    "global": "^4.3.1"
  }
}
